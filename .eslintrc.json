{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["warning"],
    "no-unused-vars": ["error"],
    "camelcase": ["error", { "properties": "always" }]
  }
}
